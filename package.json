{
  "name": "aichat-backend",
  "version": "1.0.0",
  "description": "Backend service for voucher",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc",
    "logs": "docker logs -f aichat",
    "start": "docker-compose up -d --build",
    "start:fresh": "docker-compose down -v && npm start -- --force-recreate",
    "typeorm": "NODE_ENV=cli node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create -- -n"
  },
  "author": "Lim Tze Soon",
  "license": "ISC",
  "dependencies": {
    "@js-joda/core": "^4.3.1",
    "@js-joda/timezone": "^2.10.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql2": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.14.0",
    "typeorm": "^0.2.38",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/formidable": "^2.0.0",
    "@types/lodash": "^4.14.176",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.6",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
